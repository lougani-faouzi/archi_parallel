taskset -c 3 ./dotprod_SSE_AVX $((2980 * 2**10 /2)) 1000 | cut -d';' -f1,9 > dotprod_L3.dat


taskset -c 3 ./dotprod_SSE_AVX $((3400 * 2**10 /2)) 1000| cut -d';' -f1,9 > DRAM.dat
