taskset -c 3 ./memcpy $(( 2980 * 2**10 /2)) 1000 | cut -d';' -f1,9 > memcpy_L3.dat


taskset -c 3 ./memcpy $(( 3400 * 2**10 /2)) 1000| cut -d';' -f1,9 > DRAM.dat
